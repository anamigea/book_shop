<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mainVBox" alignment="CENTER" prefHeight="332.0" prefWidth="499.0" spacing="20.0" styleClass="background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="WELCOME!" textFill="#faf7f7ba">
      <font>
         <Font name="System Bold" size="72.0" />
      </font>
   </Label>
   <Button fx:id="guestButton" alignment="CENTER" mnemonicParsing="false" onAction="#guestClick" onMouseEntered="#guestButtonOnMouseEntered" onMouseExited="#guestButtonOnMouseExited" prefHeight="35.0" prefWidth="85.0" style="-fx-background-color: #fff000;" text="GUEST" />
   <Button fx:id="librarianButton" alignment="CENTER" mnemonicParsing="false" onAction="#librarianClick" onMouseEntered="#librarianButtonOnMouseEntered" onMouseExited="#librarianButtonOnMouseExited" prefHeight="34.0" prefWidth="88.0" style="-fx-background-color: #A569BD;" text="LIBRARIAN" />
   <Button fx:id="exitButton" alignment="CENTER" mnemonicParsing="false" prefHeight="29.0" prefWidth="51.0" text="EXIT" />
</VBox>
